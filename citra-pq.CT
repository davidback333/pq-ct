local tmp, tmpSize = popDS(mrDesc("dsPersona"), "+_ptrComp")

for _, prs in spairs(com, oByArcanaLevel) do
  local r0 = mrClone(tmp, string.format("+%s*%x", tmpSize, prs.id))
