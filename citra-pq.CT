  local a = a2d[prs.arcana] or table.filter(arc, fById(prs.arcana))[1].name
  r0.Description = string.format("%s | %03d. %s [%s]", a, prs.id, prs.name, prs.tag)
  a2d[prs.arcana] = a
end

